  </PropertyGroup>
  <PropertyGroup />
  <ItemGroup />
  <ItemGroup>
    <Reference Include="System.Windows.Extensions">
      <HintPath>System.Windows.Extensions.dll</HintPath>
    </Reference>
  </ItemGroup>
</Project>